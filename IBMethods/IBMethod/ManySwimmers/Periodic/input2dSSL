// physical parameters
L = 0.025                                   // length of the computational (fluid) domain (m)
RADIUS_SMALL = 0.001            //Small sphere radius (m)
REYNOLDS_NUMBER = 2.0			//Re = rho*vel*rad/mu
FREQUENCY_OF_OSCILLATION = 10.0		//freq of osc b/w 2 discs
SURF_TO_SURF_DISTANCE = 0.002 		// dist b/w 2 discs' surfaces
MAX_AMPLITUDE = 0.002                    //
AMPLITUDE_OF_SPRING_PERCENT = 0.8	// fraction of amplitude
SMALL_AMPLITUDE = MAX_AMPLITUDE*AMPLITUDE_OF_SPRING_PERCENT*0.8     //
RHO = 1000.0                                // fluid mass density (kg / m^3)
//
//constants
PI = 3.141592653589			//
OMEGA = 2.0*PI*FREQUENCY_OF_OSCILLATION     //
MU = OMEGA*RHO*SMALL_AMPLITUDE*RADIUS_SMALL/REYNOLDS_NUMBER	//fluid viscosity
//
// grid spacing parameters
MAX_LEVELS = 4                            // maximum number of levels in locally refined grid
REF_RATIO  = 4                            // refinement ratio between levels
N = 4                                     // actual    number of grid cells on coarsest grid level
NFINEST = (REF_RATIO^(MAX_LEVELS - 1))*N  // effective number of grid cells on finest   grid level
DX = (1.0*L)/NFINEST                      // Cartesian mesh width (m)
//
// solver parameters
DELTA_FUNCTION      = "IB_4"              //
START_TIME          = 0.0e0               // initial simulation time
END_TIME            = 2.0e01               // final simulation time
GROW_DT             = 1.0e0               // growth factor for timesteps
NUM_CYCLES          = 1                   // number of cycles of fixed-point iteration
CONVECTIVE_OP_TYPE  = "PPM"               // convective differencing discretization type
CONVECTIVE_FORM     = "ADVECTIVE"         // how to compute the convective terms
NORMALIZE_PRESSURE  = TRUE                // whether to explicitly force the pressure to have mean zero
CFL_MAX             = 0.3                 // maximum CFL number
DT = 1.0e-5                               // timestep duration (s)
ERROR_ON_DT_CHANGE  = TRUE                // whether to emit an error message if the time step size changes
VORTICITY_TAGGING   = TRUE                // whether to tag cells for refinement based on vorticity thresholds
TAG_BUFFER          = 2                   // size of tag buffer used by grid generation algorithm
REGRID_CFL_INTERVAL = 0.5                 // regrid whenever any material point could have moved 0.5 meshwidths since previous regrid
OUTPUT_U            = TRUE                //
OUTPUT_P            = TRUE                //
OUTPUT_F            = FALSE                //
OUTPUT_OMEGA        = TRUE                //
OUTPUT_DIV_U        = TRUE                //
ENABLE_LOGGING      = TRUE                //
//
CartesianGeometry {                //
   domain_boxes = [ (0,0) , (5*N - 1,5*N - 1) ]                //
   x_lo = 0.0, 0.0  // lower end of computational domain.
   x_up =  0.125,  0.125  // upper end of computational domain.
   periodic_dimension = 1, 1                //
} //
//
GriddingAlgorithm { //
   allow_patches_smaller_than_minimum_size_to_prevent_overlaps = TRUE //
//
   max_levels = MAX_LEVELS           // Maximum number of levels in hierarchy.
   ratio_to_coarser { //
      level_1 = REF_RATIO,REF_RATIO  // vector ratio to next coarser level
      level_2 = REF_RATIO,REF_RATIO                //
      level_3 = REF_RATIO,REF_RATIO                //
      level_4 = REF_RATIO,REF_RATIO                //
      level_5 = REF_RATIO,REF_RATIO                //
   } //
                //
   largest_patch_size {                //
      level_0 = NFINEST, NFINEST  // largest patch allowed in hierarchy
                                  // NOTE: all finer levels will use same values as level_0...
   } //
//
   smallest_patch_size {                //
      level_0 = N, N              // smallest patch allowed in hierarchy
                                  // NOTE: all finer levels will use same values as level_0...
   }                //
//
   efficiency_tolerance   = 0.85e0    // min % of tag cells in new patch level
   combine_efficiency     = 0.85e0    // chop box if sum of volumes of smaller
                                      // boxes < efficiency * vol of large box
} //
//
IBHierarchyIntegrator {                //
   start_time          = START_TIME                //
   end_time            = END_TIME                //
   grow_dt             = GROW_DT                //
   num_cycles          = NUM_CYCLES                //
   regrid_cfl_interval = REGRID_CFL_INTERVAL                //
   dt_max              = DT                //
   error_on_dt_change  = ERROR_ON_DT_CHANGE                //
   tag_buffer          = TAG_BUFFER                //
   enable_logging      = ENABLE_LOGGING                //
} //
//
IBMethod {                //
   delta_fcn      = DELTA_FUNCTION                //
   enable_logging = ENABLE_LOGGING                //
}                //
                //
IBStandardInitializer {                //
   max_levels = MAX_LEVELS                //
   structure_names = "skeleton1", "skeleton2", "skeleton3", "skeleton4", "skeleton5", "skeleton6", "skeleton7", "skeleton8", "skeleton9", "skeleton10", "skeleton11", "skeleton12", "skeleton13", "skeleton14", "skeleton15", "skeleton16", "skeleton17", "skeleton18", "skeleton19", "skeleton20", "skeleton21", "skeleton22", "skeleton23", "skeleton24", "skeleton25", "skeleton26", "skeleton27", "skeleton28", "skeleton29", "skeleton30", "skeleton31", "skeleton32", "skeleton33", "skeleton34", "skeleton35", "skeleton36", "skeleton37", "skeleton38", "skeleton39", "skeleton40", "skeleton41", "skeleton42", "skeleton43", "skeleton44", "skeleton45", "skeleton46", "skeleton47", "skeleton48", "skeleton49", "skeleton50", "skeleton51", "skeleton52", "skeleton53", "skeleton54", "skeleton55", "skeleton56", "skeleton57", "skeleton58", "skeleton59", "skeleton60", "skeleton61", "skeleton62", "skeleton63", "skeleton64", "skeleton65", "skeleton66", "skeleton67", "skeleton68", "skeleton69", "skeleton70", "skeleton71", "skeleton72", "skeleton73", "skeleton74", "skeleton75", "skeleton76", "skeleton77", "skeleton78", "skeleton79", "skeleton80", "skeleton81", "skeleton82", "skeleton83", "skeleton84", "skeleton85", "skeleton86", "skeleton87", "skeleton88", "skeleton89", "skeleton90", "skeleton91", "skeleton92", "skeleton93", "skeleton94", "skeleton95", "skeleton96", "skeleton97", "skeleton98", "skeleton99", "skeleton100", "botup1", "botup2", "botup3", "botup4", "botup5", "botup6", "botup7", "botup8", "botup9", "botup10", "botup11", "botup12", "botup13", "botup14", "botup15", "botup16", "botup17", "botup18", "botup19", "botup20", "botup21", "botup22", "botup23", "botup24", "botup25", "botup26", "botup27", "botup28", "botup29", "botup30", "botup31", "botup32", "botup33", "botup34", "botup35", "botup36", "botup37", "botup38", "botup39", "botup40", "botup41", "botup42", "botup43", "botup44", "botup45", "botup46", "botup47", "botup48", "botup49", "botup50", "botup51", "botup52", "botup53", "botup54", "botup55", "botup56", "botup57", "botup58", "botup59", "botup60", "botup61", "botup62", "botup63", "botup64", "botup65", "botup66", "botup67", "botup68", "botup69", "botup70", "botup71", "botup72", "botup73", "botup74", "botup75", "botup76", "botup77", "botup78", "botup79", "botup80", "botup81", "botup82", "botup83", "botup84", "botup85", "botup86", "botup87", "botup88", "botup89", "botup90", "botup91", "botup92", "botup93", "botup94", "botup95", "botup96", "botup97", "botup98", "botup99", "botup100", "botlow1", "botlow2", "botlow3", "botlow4", "botlow5", "botlow6", "botlow7", "botlow8", "botlow9", "botlow10", "botlow11", "botlow12", "botlow13", "botlow14", "botlow15", "botlow16", "botlow17", "botlow18", "botlow19", "botlow20", "botlow21", "botlow22", "botlow23", "botlow24", "botlow25", "botlow26", "botlow27", "botlow28", "botlow29", "botlow30", "botlow31", "botlow32", "botlow33", "botlow34", "botlow35", "botlow36", "botlow37", "botlow38", "botlow39", "botlow40", "botlow41", "botlow42", "botlow43", "botlow44", "botlow45", "botlow46", "botlow47", "botlow48", "botlow49", "botlow50", "botlow51", "botlow52", "botlow53", "botlow54", "botlow55", "botlow56", "botlow57", "botlow58", "botlow59", "botlow60", "botlow61", "botlow62", "botlow63", "botlow64", "botlow65", "botlow66", "botlow67", "botlow68", "botlow69", "botlow70", "botlow71", "botlow72", "botlow73", "botlow74", "botlow75", "botlow76", "botlow77", "botlow78", "botlow79", "botlow80", "botlow81", "botlow82", "botlow83", "botlow84", "botlow85", "botlow86", "botlow87", "botlow88", "botlow89", "botlow90", "botlow91", "botlow92", "botlow93", "botlow94", "botlow95", "botlow96", "botlow97", "botlow98", "botlow99", "botlow100"               //
